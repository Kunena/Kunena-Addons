    <config>
        <fields name="params">
            <fieldset name="basic">
                <field name="ksearch_width" type="text" default="20" label="MOD_KUNENASEARCH_BOX_WIDTH"
                       description="MOD_KUNENASEARCH_BOX_WIDTH_DESC"/>
                <field name="ksearch_txt" type="text" default="" label="MOD_KUNENASEARCH_PARAMTEXT"
                       description="MOD_KUNENASEARCH_PARAMTEXT_DESC"/>
                <field name="ksearch_button" type="radio" default="" label="MOD_KUNENASEARCH_SEARCHBUTTON"
                       description="MOD_KUNENASEARCH_SEARCHBUTTON_DESC">
                    <option value="">MOD_KUNENASEARCH_HIDE</option>
                    <option value="1">MOD_KUNENASEARCH_SHOW</option>
                </field>
                <field name="ksearch_button_pos" type="list" default="right"
                       label="MOD_KUNENASEARCH_SEARCHBUTTON_POSITION"
                       description="MOD_KUNENASEARCH_SEARCHBUTTON_POSITION_DESC">
                    <option value="right">MOD_KUNENASEARCH_RIGHT</option>
                    <option value="bottom">MOD_KUNENASEARCH_BOTTOM</option>
                </field>
                <field name="ksearch_button_txt" type="text" default="Search" label="MOD_KUNENASEARCH_SEARCHBUTTON_TEXT"
                       description="MOD_KUNENASEARCH_SEARCHBUTTON_TEXT_DESC"/>
            </fieldset>

            <fieldset name="advanced">
                <field
                        name="moduleclass_sfx"
                        type="text"
                        label="COM_MODULES_FIELD_MODULECLASS_SFX_LABEL"
                        description="COM_MODULES_FIELD_MODULECLASS_SFX_DESC"/>

                <field
                        name="owncache"
                        type="list"
                        default="1"
                        label="COM_MODULES_FIELD_CACHING_LABEL"
                        description="COM_MODULES_FIELD_CACHING_DESC">
                    <option
                            value="1">JGLOBAL_USE_GLOBAL
                    </option>
                    <option
                            value="0">COM_MODULES_FIELD_VALUE_NOCACHING
                    </option>
                </field>

                <field
                        name="cache_time"
                        type="text"
                        default="180"
                        label="COM_MODULES_FIELD_CACHE_TIME_LABEL"
                        description="COM_MODULES_FIELD_CACHE_TIME_DESC"/>
            </fieldset>
        </fields>
